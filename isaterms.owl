    <!-- http://purl.org/isaterms/ISA_dataset -->

    <owl:Class rdf:about="http://purl.org/isaterms/ISA_dataset">
        <rdfs:subClassOf rdf:resource="http://purl.org/isaterms/information"/>
        <terms:description>An ISA dataset is contains the metadata corresponding to the description of an experiment and it is divied into the metadata for an investigation, associated studies and associated assays. The ISA dataset aims at capturing and communicating the complex metadata required to interpret experiments employing a combination of technologies, and the associated data files.

The ISA dataset has to comply with the ISA specification available at the ISA tools website (http://isa-tools.org/). It can have distributions in tabular format (ISAtab distribution) or following the semantic web/linked data approach (ISAowl distribution), as generated by the ISA2OWL project.</terms:description>
        <isaterms:is_a>dctypes:Dataset;http://purl.org/dc/dcmitype/Dataset</isaterms:is_a>
        <rdfs:label>ISA dataset</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/isaterms/ISAowl_distribution -->

    <owl:Class rdf:about="http://purl.org/isaterms/ISAowl_distribution">
        <rdfs:subClassOf rdf:resource="http://purl.org/isaterms/information"/>
        <terms:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An ISAowl distribution represents the RDF/OWL form of an ISA dataset when generated by the ISA2OWL converter.</terms:description>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ISAowl distribution</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/isaterms/ISArdf_distribution -->

    <owl:Class rdf:about="http://purl.org/isaterms/ISArdf_distribution">
        <rdfs:subClassOf rdf:resource="http://purl.org/isaterms/information"/>
        <terms:description>An ISArdf distribution represents the RDF/OWL form of an ISA dataset when generated by the ISA2RDF converter.</terms:description>
        <rdfs:label>ISArdf distribution</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/isaterms/ISAtab_distribution -->

    <owl:Class rdf:about="http://purl.org/isaterms/ISAtab_distribution">
        <rdfs:subClassOf rdf:resource="http://purl.org/isaterms/information"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/isaterms/has_part"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/isaterms/assay_file"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/isaterms/has_part"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/isaterms/study_file"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/isaterms/has_part"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://purl.org/isaterms/investigation_file"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <terms:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An ISAtab distribution represents the tabular form of an ISA dataset. It could have been generated using ISAcreator, OntoMaton or any other spreadsheet software.</terms:description>
        <rdfs:label xml:lang="en">ISAtab distribution</rdfs:label>
    </owl:Class>
    


