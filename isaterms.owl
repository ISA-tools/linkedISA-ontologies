    <owl:Ontology rdf:about="http://purl.org/isaterms">
        <owl:versionIRI rdf:resource="http://purl.org/isaterms/1.0"/>
        <terms:creator>Alejandra Gonzalez-Beltran</terms:creator>
        <terms:creator>Philippe Rocca-Serra</terms:creator>
        <terms:description>This ontology defines the vocabulary used to describe ISA-TAB datasets. This vocabulary is mapped to external ontologies, mainly the Ontology for Biomedical Investigations (OBI).</terms:description>
        <terms:rights>This Ontology is distributed under a Creative Commons Attribution License - http://creativecommons.org/licenses/by/3.0/</terms:rights>
        <terms:title>The ISA ontology, where ISA stands for Investigation/Study/Assay.</terms:title>
        <rdfs:seeAlso>http://isa-tools.org</rdfs:seeAlso>
        <owl:versionInfo>1.0</owl:versionInfo>
    </owl:Ontology>
    


