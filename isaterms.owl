    <!-- http://purl.org/isaterms/assay -->

    <owl:Class rdf:about="http://purl.org/isaterms/assay">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/isaterms/has_measurement"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/isaterms/measurement"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/isaterms/has_technology"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/isaterms/technology"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://purl.org/isaterms/process"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/isaterms/has_ISA_file"/>
